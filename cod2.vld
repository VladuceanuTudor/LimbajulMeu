inceput 
{
    intreg var;
    citeste var;
    
    cat timp var > 0 executa 
    {
        daca var < 100 atunci {afiseaza var " ";}
        var = var - 1;
        //afiseaza x " ";
    }

    real x;

    citeste x;
    x = (intreg)x;

    afiseaza "cast la intreg: " x;

    afiseaza "\n";
    afiseaza "citeste y: ";
    intreg y;
    citeste y;
    x = (real)y/2;

    afiseaza "cast la real: " x "\n";

    x = (intreg)x/2;

    afiseaza "cast la intreg: " x;
}
